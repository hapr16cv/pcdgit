#include<stdio.h>
#include<math.h>
float square(float n)
{
  int s,i;
  if(n>0)
  {
    for(i=0;i<n;i++)
    {
      s=(s+(n/s)).2;
    }
  return s;
}
int main()
{
  int n;
  float sq;
  printf("enter the number : ");
  scanf("%d",&sq);
  sq=square(n);
  printf("the square root of %d is : %f ",n,sq);
  return 0;
}
